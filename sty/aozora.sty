	\def\rubykatuji{\rubyfamily\small}
	\vbox to\textheight\bgroup\tate\hsize\textwidth
		\vfil
		{\centering\Huge\chapterfont #1\par}%
		\vfil
	\egroup
	\endgroup
	\markboth{}{#1}
	\addcontentsline{toc}{chapter}{#1}
	\if@restonecol\twocolumn \else \newpage \fi
}
